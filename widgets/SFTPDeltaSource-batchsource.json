{
  "metadata": {
    "spec-version": "1.5"
  },
  "display-name": "SFTPDelta",
  "icon": {
       "type": "inline",
       "arguments": {
           "data": "data:image/svg+xml;base64,"
       }
  },
  "configuration-groups": [
    {
      "label": "Reference",
      "properties": [
        {
          "label": "Reference Name",
          "widget-type": "textbox",
          "name": "referenceName",
          "widget-attributes": {
            "placeholder": "Name used to identify this source for lineage",
            "default": ""
          }
        }
      ]
    },
    {
      "label": "SFTP Server",
      "properties": [
        {
          "label": "Host",
          "name": "sftpServer",
          "widget-type": "textbox",
          "widget-attributes": {
            "default": ""
          }
        },
        {
          "label": "Port",
          "name": "sftpPort",
          "widget-type": "textbox",
          "widget-attributes": {
            "default": "22"
          }
        },
        {
          "label": "Path",
          "name": "sftpPath",
          "widget-type": "textbox",
          "widget-attributes": {
            "default": ""
          }
        },
        {
          "widget-type": "radio-group",
          "label": "Recursive",
          "name": "recursive",
          "widget-attributes": {
            "placeholder": "Recursive read",
            "layout": "inline",
            "default": "no",
            "options": [
              {
                "id": "no",
                "label": "No"
              },
              {
                "id": "yes",
                "label": "Yes"
              }
            ]
          }
        },
        {
          "label": "Regex Path Filter ",
          "name": "regexFilter",
          "widget-type": "textbox",
          "widget-attributes": {
            "default": "",
            "placeholder": "Regex Path Filter. For Example: .*.parquet 'https://docs.oracle.com/javase/8/docs/api/java/util/regex/Pattern.html#sum'"
          }
        },
        {
          "label": "Username",
          "name": "sftpUser",
          "widget-type": "textbox",
          "widget-attributes": {
            "default": ""
          }
        },
        {
          "label": "Authentication",
          "name": "authentication",
          "widget-type": "radio-group",
          "widget-attributes": {
            "layout": "inline",
            "default": "privatekey",
            "options": [
              {
                "id": "privatekey",
                "label": "PrivateKey"
              },
              {
                "id": "password",
                "label": "Password"
              }
            ]
          }
        },
        {
          "widget-type": "password",
          "label": "Password",
          "name": "sftpPass"
        },
        {
          "widget-type": "textarea",
          "label": "PrivateKey",
          "name": "PrivateKey"
        },
        {
          "widget-type": "password",
          "label": "PrivateKey Passphrase",
          "name": "PrivateKeyPass"
        }
      ]
    },
    {
      "label": "Delta",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Last Date Processed File",
          "name": "persistDelta",
          "widget-attributes": {
            "default": "",
            "placeholder": "The last modification time file to persist"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Last Date Processed",
          "name": "fromDelta",
          "widget-attributes": {
            "placeholder": "The last modification time. They are represented as seconds from Jan 1, 1970 in UTC"
          }
        }
      ]
    },
    {
      "label": "Proxy",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Proxy IP",
          "name": "proxyIP"
        },
        {
          "widget-type": "textbox",
          "label": "Proxy Port",
          "name": "proxyPort"
        }
      ]
    },
    {
      "label" : "GCS Delta properties",
      "properties" : [
        {
          "name": "serviceAccountType",
          "label": "Service Account Type",
          "widget-type": "radio-group",
          "widget-attributes": {
            "layout": "inline",
            "default": "filePath",
            "options": [
              {
                "id": "filePath",
                "label": "File Path"
              },
              {
                "id": "json",
                "label": "JSON"
              }
            ]
          }
        },
        {
          "widget-type": "textbox",
          "label": "GCP Service Account JSON",
          "name": "serviceAccountJSON"
        },
        {
          "widget-type": "textbox",
          "label": " GCP Service Account File Path",
          "name": "serviceFilePath",
          "widget-attributes": {
            "default": "auto-detect"
          }
        }
      ]
    },
    {
      "label": "Advanced",
      "properties": [
        {
          "widget-type": "keyvalue",
          "label": "Properties for SSH",
          "name": "sshProperties",
          "widget-attributes": {
            "showDelimiter": "false",
            "key-placeholder": "key",
            "value-placeholder": "value"
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "widget-type": "non-editable-schema-editor",
      "schema": {
        "name": "etlSchemaBody",
        "type": "record",
        "fields": [
          {
            "name": "connection",
            "type": "string"
          },
          {
            "name": "fullfilename",
            "type": "string"
          },
          {
            "name": "filename",
            "type": "string"
          },
          {
            "name": "basename",
            "type": "string"
          },
          {
            "name": "size",
            "type": "long"
          },
          {
            "name": "mtime",
            "type": "int"
          }
        ]
      }
    }
  ],
  "filters": [
    {
      "name": "AuthByPrivateKey",
      "condition": {
        "expression": "authentication == 'privatekey'"
      },
      "show":[
        {
          "type": "property",
          "name": "PrivateKey"
        },
        {
          "type": "property",
          "name": "PrivateKeyPass"
        }
      ]
    },
    {
      "name": "AuthByPassword",
      "condition": {
        "expression": "authentication == 'password'"
      },
      "show":
      [
        {
          "type": "property",
          "name": "sftpPass"
        }
      ]
    },
    {
      "name": "ServiceAuthenticationTypeFilePath",
      "condition": {
        "expression": "serviceAccountType == 'filePath'"
      },
      "show":[
        {
          "type": "property",
          "name": "serviceFilePath"
        }
      ]
    },
    {
      "name": "ServiceAuthenticationTypeJSON",
      "condition": {
        "expression": "serviceAccountType == 'json'"
      },
      "show":[
        {
          "type": "property",
          "name": "serviceAccountJSON"
        }
      ]
    }
  ],

  "jump-config": {
    "datasets": [
      {
        "ref-property-name": "referenceName"
      }
    ]
  }

}